{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"lib": [
			"ESNext",
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"paths": {
			"@pup007/artipub": [
				"../dist"
			]
		},
		"types": [
			"node",
		]
	},
	"include": [
		"./src/**/*.ts",
	]
}
